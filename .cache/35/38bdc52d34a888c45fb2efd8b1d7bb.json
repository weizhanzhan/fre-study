break\r\n      case 'style':\r\n        node.style.cssText = value\r\n        break\r\n      default:\r\n        node.setAttribute(name, value)\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"83e85cd07fc6d53c997d42a1b2cbc9ae","cacheData":{"env":{}}}